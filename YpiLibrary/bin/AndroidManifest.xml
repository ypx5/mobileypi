            android:label="@string/title_activity_personnainfo" >
        </activity>
        <activity
            android:name="com.flashzhang.ypilibrary.BorrowinfoActivity"
            android:label="@string/title_activity_borrowinfo" >
        </activity>
        <activity
            android:name="com.flashzhang.ypilibrary.MoreinfoActivity"
            android:label="@string/title_activity_moreinfo" >
        </activity>
        <activity
            android:name="com.flashzhang.ypilibrary.MarksinfoActivity"
            android:label="@string/title_activity_marksinfo" >
        </activity>
        <activity
            android:name="com.flashzhang.ypilibrary.CardinfoActivity"
